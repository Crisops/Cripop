{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/layouts": [
        "src/layouts/*"
      ],
      "@/components": [
        "src/components/*"
      ],
      "@/icons/*": [
        "src/icons/*"
      ],
      "@/types/*": [
        "src/types/*"
      ],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}